<div class="col-xs-10  col-xs-offset-1 col-md-8 col-md-offset-2">
  <div>
  <%= search_form_for @q do |f| %>
  <table class="table table-striped">
    <tr>
      <th><%= f.label :name_cont, "お店の名前" %></th>
      <td><%= f.text_field :name_cont %></td>
    </tr>
    <tr>
      <th><%= f.label :address_cont, "住所" %></th>
      <td> <%= f.text_field :address_cont %></td>
    </tr>
  </table>
    <div class="actions"><%= f.submit "検索" %></div>
  <% end %>
  <%= render partial: 'index', locals: {articles: @articles}%>
  </div>
</div>
